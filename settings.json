                    {"name":"pronto", "version":"2.1.0"},
                    {"name":"ujson", "version":"5.1.0"},
                    {"name":"pyparsing", "version":"3.0.6"},
                    {"name":"owlready2", "version":"0.36"}
                ]
            },
            {
                "source": "https://$GWS_PIP_PACKAGE_TOKEN@gitlab.com/api/v4/projects/27171024/packages/pypi",
