        "gws_biota:mariadb_url"     : ""
    },
    "technical_info": {
      "MARIA_DB_URL" : "https://storage.gra.cloud.ovh.net/v1/AUTH_a0286631d7b24afba3f3cdebed2992aa/gws_biota/db/0.4.0/mariadb.zip"
